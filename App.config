<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="ConfigSettings" type="AlertO365.AppConfig, AlertO365"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <ConfigSettings ServiceURL="https://office365servicehealthcommunications.cloudapp.net/shdtenantcommunications.svc" 
                  DomainNames="domain.com" 
                  UserName="user@jdomain.com" 
                  Password="your password here" 
                  IsAOBO="0" 
                  PastDays="1"
                  SenderAddress="O365_Check@x.y"
                  RecipientAddress="admin@x.y"
                  SMTPServer="localhost"
                  MailSubject="Office365 Events Check"
                  Company="(Company/Domain Name)"
                  SendViaEmail="Yes"
                  />
</configuration>
